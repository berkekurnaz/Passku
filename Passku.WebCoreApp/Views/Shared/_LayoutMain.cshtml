
@{
    Layout = null;
}

<!DOCTYPE html>

<html>
<head>
    <title>Passku - Password Creator and Management App</title>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1, shrink-to-fit=no">


    <link href="https://fonts.googleapis.com/css?family=Montserrat:300,400,700" rel="stylesheet">
    <link rel="stylesheet" href="~/TemplateMain/fonts/icomoon/style.css">

    <link rel="stylesheet" href="~/TemplateMain/css/bootstrap.min.css">
    <link rel="stylesheet" href="~/TemplateMain/css/jquery-ui.css">
    <link rel="stylesheet" href="~/TemplateMain/css/owl.carousel.min.css">
    <link rel="stylesheet" href="~/TemplateMain/css/owl.theme.default.min.css">
    <link rel="stylesheet" href="~/TemplateMain/css/owl.theme.default.min.css">

    <link rel="stylesheet" href="~/TemplateMain/css/jquery.fancybox.min.css">

    <link rel="stylesheet" href="~/TemplateMain/css/bootstrap-datepicker.css">

    <link rel="stylesheet" href="~/TemplateMain/fonts/flaticon/font/flaticon.css">

    <link rel="stylesheet" href="~/TemplateMain/css/aos.css">

    <link rel="stylesheet" href="~/TemplateMain/css/style.css">
</head>
<body data-spy="scroll" data-target=".site-navbar-target" data-offset="300">

    <div class="site-wrap" id="home-section">

        <div class="site-mobile-menu site-navbar-target">
            <div class="site-mobile-menu-header">
                <div class="site-mobile-menu-close mt-3">
                    <span class="icon-close2 js-menu-toggle"></span>
                </div>
            </div>
            <div class="site-mobile-menu-body"></div>
        </div>

        <div class="container d-none d-lg-block">
            <div class="row">
                <div class="col-12 text-center mb-4 mt-5">
                    <h1 class="mb-0 site-logo"><a href="/Home/Index/" class="text-black h2 mb-0">Passku</a></h1>
                </div>
            </div>
        </div>
        <header class="site-navbar py-md-4 js-sticky-header site-navbar-target" role="banner">

            <div class="container">
                <div class="row align-items-center">

                    <div class="col-6 col-md-6 col-xl-2  d-block d-lg-none">
                        <h1 class="mb-0 site-logo"><a href="/Home/Index/" class="text-black h2 mb-0">Passku</a></h1>
                    </div>

                    <div class="col-12 col-md-10 main-menu">
                        <nav class="site-navigation position-relative text-right" role="navigation">

                            <ul class="site-menu main-menu js-clone-nav mr-auto d-none d-lg-block">
                                <li><a href="/Home/Index/" class="nav-link">Home</a></li>
                                <li><a href="/Home/Create/" class="nav-link">Create Password</a></li>
                                <li><a href="/Home/About/" class="nav-link">About</a></li>
                                <li><a href="/Home/Source/" class="nav-link">Source Code</a></li>
                                <li><a href="/Home/Contact/" class="nav-link">Contact</a></li>
                                <li><a href="/User/Login/" class="nav-link">Login</a></li>
                            </ul>
                        </nav>
                    </div>


                    <div class="col-6 col-md-6 d-inline-block d-lg-none ml-md-0"><a href="#" class="site-menu-toggle js-menu-toggle text-black float-right"><span class="icon-menu h3"></span></a></div>

                </div>
            </div>

        </header>

         @RenderBody()

        <div class="footer py-5 border-top text-center">
            <div class="container">
                <div class="row mb-5">
                    <div class="col-12">
                        <p class="mb-0">
                            <a href="#" class="p-3"><span class="icon-facebook"></span></a>
                            <a href="#" class="p-3"><span class="icon-twitter"></span></a>
                            <a href="#" class="p-3"><span class="icon-instagram"></span></a>
                        </p>
                    </div>
                </div>
                <div class="row">
                    <div class="col-md-12">
                        <p class="mb-0">
                            Passku is an open source password generation and password management platform.<br />
                            <span>Kurnaz Teknoloji</span>
                        </p>
                    </div>
                </div>
            </div>
        </div>




    </div> <!-- .site-wrap -->


    <script src="~/TemplateMain/js/jquery-3.3.1.min.js"></script>
    <script src="~/TemplateMain/js/jquery-migrate-3.0.1.min.js"></script>
    <script src="~/TemplateMain/js/jquery-ui.js"></script>
    <script src="~/TemplateMain/js/popper.min.js"></script>
    <script src="~/TemplateMain/js/bootstrap.min.js"></script>
    <script src="~/TemplateMain/js/owl.carousel.min.js"></script>
    <script src="~/TemplateMain/js/jquery.stellar.min.js"></script>
    <script src="~/TemplateMain/js/jquery.countdown.min.js"></script>
    <script src="~/TemplateMain/js/bootstrap-datepicker.min.js"></script>
    <script src="~/TemplateMain/js/jquery.easing.1.3.js"></script>
    <script src="~/TemplateMain/js/aos.js"></script>
    <script src="~/TemplateMain/js/jquery.fancybox.min.js"></script>
    <script src="~/TemplateMain/js/jquery.sticky.js"></script>


    <script src="~/TemplateMain/js/main.js"></script>

</body>
</html>
